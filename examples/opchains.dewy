% in dewy you can combine arithmetic operators into an opchain
x = 2
y = 3
z = 4
printl(+x)          % 0+x
printl(/x)          % 1/x
printl(/-x)         % 1/-x
printl(-/x)         % 0-1/x
printl(-x^2)        % 0-x^2
printl(/x^2)        % 1/x^2
printl(/x-2)        % 1/x-2
printl(-x/2)        % 0-x/2
printl(25-/2)       % 25-1/2
printl(25+-/2)      % 25+0-1/2
printl(2^/-+*32)    % 2^(1/(0-(0+(1*32))))

printl(x*+3)
printl(y/-4)
printl(z^/2)

printl(10^/-2)

printl(100^/2)
printl(5+-1)