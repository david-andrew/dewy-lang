multiples = [3 -> 'Fizz' 5 -> 'Buzz' #{7 -> 'Bazz' 11 -> 'Bar'}#]
loop i in [0..100)
{
    printed_words = false
    loop [multiple word] in multiples
    {
        if i % multiple =? 0
        {
            print(word)
            printed_words = true
        }
    }
    if not printed_words print(i)
    printl
}
