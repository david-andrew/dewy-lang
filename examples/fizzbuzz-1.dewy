% fizbuzz that works with the current version of dewy
multiples = [3 5]
words = ['Fizz' 'Buzz']
loop i in [0..100)
{
    printed_words = false
    loop multiple in multiples and word in words
    {
        if i % multiple =? 0 
        { 
            print(word)
            printed_words = true
        }
    }
    if not printed_words print(i)
    printl()
}